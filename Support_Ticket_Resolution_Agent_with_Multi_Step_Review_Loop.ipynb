        "# Support Ticket Resolution Agent with Multi-Step Review Loop\n",
        "# Built with LangGraph Framework\n",
        "\n",
        "import os\n",
        "import csv\n",
        "import json\n",
        "import logging\n",
        "from datetime import datetime\n",
        "from typing import Dict, List, Any, Optional, TypedDict\n",
        "from dataclasses import dataclass, asdict\n",
        "\n",
        "# LangGraph and LangChain imports\n",
        "from langgraph.graph import StateGraph, END\n",
        "from langgraph.prebuilt import ToolNode\n",
        "from langchain_groq import ChatGroq\n",
        "from langchain_community.vectorstores import FAISS\n",
        "from langchain_community.embeddings import HuggingFaceEmbeddings\n",
        "from langchain_core.messages import HumanMessage, AIMessage\n",
        "from langchain_core.prompts import ChatPromptTemplate\n",
        "from langchain_text_splitters import RecursiveCharacterTextSplitter\n",
        "from langchain_core.documents import Document\n",
        "\n",
        "# Install required packages (run in Colab)\n",
        "# !pip install langgraph langchain langchain-groq langchain-community faiss-cpu sentence-transformers\n",
        "\n",
        "# Set up logging\n",
        "logging.basicConfig(level=logging.INFO)\n",
        "logger = logging.getLogger(__name__)\n",
        "\n",
        "# Configuration\n",
        "GROQ_API_KEY = \"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\"  # Replace with your Groq API key\n",
        "MAX_REVIEW_ATTEMPTS = 2\n",
        "\n",
        "@dataclass\n",
        "class SupportTicket:\n",
        "    \"\"\"Data structure for support tickets\"\"\"\n",
        "    subject: str\n",
        "    description: str\n",
        "    ticket_id: Optional[str] = None\n",
        "    timestamp: Optional[str] = None\n",
        "\n",
        "class AgentState(TypedDict):\n",
        "    \"\"\"State management for the agent graph\"\"\"\n",
        "    ticket: SupportTicket\n",
        "    classification: str\n",
        "    context: List[str]\n",
        "    draft_response: str\n",
        "    review_result: Dict[str, Any]\n",
        "    review_attempts: int\n",
        "    final_response: str\n",
        "    escalated: bool\n",
        "    reviewer_feedback: str\n",
        "\n",
        "class SupportTicketAgent:\n",
        "    \"\"\"Main support ticket resolution agent using LangGraph\"\"\"\n",
        "\n",
        "    def __init__(self, groq_api_key: str):\n",
        "        self.groq_api_key = groq_api_key\n",
        "        os.environ[\"GROQ_API_KEY\"] = groq_api_key\n",
        "\n",
        "        # Initialize LLM with Groq (try multiple models for robustness)\n",
        "        self.llm = self._initialize_groq_llm()\n",
        "\n",
        "        # Initialize free embeddings using HuggingFace\n",
        "        self.embeddings = HuggingFaceEmbeddings(\n",
        "            model_name=\"sentence-transformers/all-MiniLM-L6-v2\"  # Free, lightweight model\n",
        "        )\n",
        "\n",
        "        # Initialize knowledge bases for each category\n",
        "        self.knowledge_bases = self._initialize_knowledge_bases()\n",
        "\n",
        "        # Build the graph\n",
        "        self.graph = self._build_graph()\n",
        "\n",
        "    def _initialize_groq_llm(self):\n",
        "        \"\"\"Initialize Groq LLM with fallback to multiple models\"\"\"\n",
        "        # Current Groq models (as of 2025) - ordered by preference\n",
        "        models_to_try = [\n",
        "            \"llama-3.1-8b-instant\",      # Fast, good for production\n",
        "            \"llama-3.3-70b-versatile\",   # More capable but slower\n",
        "            \"gemma2-9b-it\",              # Google's Gemma\n",
        "            \"llama3-groq-8b-8192-tool-use-preview\",  # Tool-focused\n",
        "            \"llama3-8b-8192\"             # Legacy fallback\n",
        "        ]\n",
        "\n",
        "        for model in models_to_try:\n",
        "            try:\n",
        "                logger.info(f\"Trying to initialize model: {model}\")\n",
        "                llm = ChatGroq(\n",
        "                    model=model,\n",
        "                    temperature=0.3,\n",
        "                    groq_api_key=self.groq_api_key,\n",
        "                    max_tokens=1024\n",
        "                )\n",
        "\n",
        "                # Test the model\n",
        "                test_response = llm.invoke(\"Hello\")\n",
        "                logger.info(f\"‚úÖ Successfully initialized model: {model}\")\n",
        "                return llm\n",
        "\n",
        "            except Exception as e:\n",
        "                logger.warning(f\"‚ùå Model {model} failed: {str(e)[:100]}...\")\n",
        "                continue\n",
        "\n",
        "        # If all models fail, raise an error with helpful message\n",
        "        raise Exception(\"\"\"\n",
        "‚ùå Could not initialize any Groq model. Please check:\n",
        "1. Your Groq API key is valid (get it at https://console.groq.com/)\n",
        "2. Your account has available credits\n",
        "3. The models are currently available\n",
        "\n",
        "Try running the setup cell again or use the local version instead.\n",
        "        \"\"\".strip())\n",
        "\n",
        "    def _initialize_knowledge_bases(self) -> Dict[str, FAISS]:\n",
        "        \"\"\"Initialize mock knowledge bases for different categories\"\"\"\n",
        "        knowledge_bases = {}\n",
        "\n",
        "        # Mock knowledge for each category\n",
        "        category_docs = {\n",
        "            \"Billing\": [\n",
        "                \"Billing inquiries can be resolved by checking account status. Refunds require manager approval.\",\n",
        "                \"Payment methods can be updated in account settings. Contact billing team for disputes.\",\n",
        "                \"Invoice questions should include account number and billing period for faster resolution.\",\n",
        "                \"Automatic billing failures often occur due to expired credit cards or insufficient funds.\"\n",
        "            ],\n",
        "            \"Technical\": [\n",
        "                \"Login issues can often be resolved by clearing browser cache and cookies.\",\n",
        "                \"Password reset links expire after 24 hours. Request new link if needed.\",\n",
        "                \"Server maintenance occurs every Sunday 2-4 AM EST. Plan accordingly.\",\n",
        "                \"Mobile app crashes can be fixed by updating to the latest version.\",\n",
        "                \"API rate limits are 1000 requests per hour. Contact support for higher limits.\"\n",
        "            ],\n",
        "            \"Security\": [\n",
        "                \"Security incidents must be escalated to security team immediately.\",\n",
        "                \"Password policies require 8+ characters with mixed case and symbols.\",\n",
        "                \"Two-factor authentication is required for admin accounts.\",\n",
        "                \"Suspicious activity should be reported within 24 hours.\",\n",
        "                \"Account lockouts occur after 5 failed login attempts.\"\n",
        "            ],\n",
        "            \"General\": [\n",
        "                \"General inquiries are handled within 24-48 hours during business days.\",\n",
        "                \"Feature requests are reviewed monthly by product team.\",\n",
        "                \"Account upgrades and downgrades take effect at next billing cycle.\",\n",
        "                \"Documentation is available at support.company.com/docs.\"\n",
        "            ]\n",
        "        }\n",
        "\n",
        "        # Create FAISS vector stores for each category\n",
        "        for category, docs in category_docs.items():\n",
        "            documents = [Document(page_content=doc, metadata={\"category\": category})\n",
        "                        for doc in docs]\n",
        "            knowledge_bases[category] = FAISS.from_documents(documents, self.embeddings)\n",
        "\n",
        "        return knowledge_bases\n",
        "\n",
        "    def _build_graph(self) -> StateGraph:\n",
        "        \"\"\"Build the LangGraph workflow\"\"\"\n",
        "        workflow = StateGraph(AgentState)\n",
        "\n",
        "        # Add nodes\n",
        "        workflow.add_node(\"classify\", self.classify_ticket)\n",
        "        workflow.add_node(\"retrieve_context\", self.retrieve_context)\n",
        "        workflow.add_node(\"draft_response\", self.draft_response)\n",
        "        workflow.add_node(\"review_draft\", self.review_draft)\n",
        "        workflow.add_node(\"refine_context\", self.refine_context)\n",
        "        workflow.add_node(\"escalate\", self.escalate_ticket)\n",
        "        workflow.add_node(\"finalize\", self.finalize_response)\n",
        "\n",
        "        # Define the workflow edges\n",
        "        workflow.set_entry_point(\"classify\")\n",
        "\n",
        "        # Linear flow for initial processing\n",
        "        workflow.add_edge(\"classify\", \"retrieve_context\")\n",
        "        workflow.add_edge(\"retrieve_context\", \"draft_response\")\n",
        "        workflow.add_edge(\"draft_response\", \"review_draft\")\n",
        "\n",
        "        # Conditional routing based on review result\n",
        "        workflow.add_conditional_edges(\n",
        "            \"review_draft\",\n",
        "            self.should_retry_or_finalize,\n",
        "            {\n",
        "                \"retry\": \"refine_context\",\n",
        "                \"finalize\": \"finalize\",\n",
        "                \"escalate\": \"escalate\"\n",
        "            }\n",
        "        )\n",
        "\n",
        "        # Retry loop\n",
        "        workflow.add_edge(\"refine_context\", \"draft_response\")\n",
        "\n",
        "        # End nodes\n",
        "        workflow.add_edge(\"finalize\", END)\n",
        "        workflow.add_edge(\"escalate\", END)\n",
        "\n",
        "        return workflow.compile()\n",
        "\n",
        "    def classify_ticket(self, state: AgentState) -> Dict[str, Any]:\n",
        "        \"\"\"Classify the support ticket into predefined categories\"\"\"\n",
        "        logger.info(\"Classifying ticket...\")\n",
        "\n",
        "        classification_prompt = ChatPromptTemplate.from_messages([\n",
        "            (\"system\", \"\"\"You are a support ticket classifier. Classify the following ticket into exactly ONE of these categories:\n",
        "            - Billing\n",
        "            - Technical\n",
        "            - Security\n",
        "            - General\n",
        "\n",
        "            Consider both the subject and description. Choose the most appropriate category.\n",
        "            Respond with only the category name (e.g., \"Technical\").\"\"\"),\n",
        "            (\"human\", \"Subject: {subject}\\nDescription: {description}\")\n",
        "        ])\n",
        "\n",
        "        try:\n",
        "            response = self.llm.invoke(\n",
        "                classification_prompt.format_messages(\n",
        "                    subject=state[\"ticket\"].subject,\n",
        "                    description=state[\"ticket\"].description\n",
        "                )\n",
        "            )\n",
        "\n",
        "            classification = response.content.strip()\n",
        "\n",
        "            # Validate classification\n",
        "            valid_categories = [\"Billing\", \"Technical\", \"Security\", \"General\"]\n",
        "            if classification not in valid_categories:\n",
        "                classification = \"General\"  # Default fallback\n",
        "\n",
        "            logger.info(f\"Ticket classified as: {classification}\")\n",
        "\n",
        "            return {\n",
        "                **state,\n",
        "                \"classification\": classification,\n",
        "                \"review_attempts\": 0\n",
        "            }\n",
        "\n",
        "        except Exception as e:\n",
        "            logger.error(f\"Classification failed: {e}\")\n",
        "            return {\n",
        "                **state,\n",
        "                \"classification\": \"General\",  # Safe fallback\n",
        "                \"review_attempts\": 0\n",
        "            }\n",
        "\n",
        "    def retrieve_context(self, state: AgentState) -> Dict[str, Any]:\n",
        "        \"\"\"Retrieve relevant context based on classification using RAG\"\"\"\n",
        "        logger.info(f\"Retrieving context for category: {state['classification']}\")\n",
        "\n",
        "        try:\n",
        "            # Get the relevant knowledge base\n",
        "            kb = self.knowledge_bases.get(state[\"classification\"])\n",
        "            if not kb:\n",
        "                logger.warning(f\"No knowledge base for category: {state['classification']}\")\n",
        "                return {**state, \"context\": []}\n",
        "\n",
        "            # Create search query from ticket\n",
        "            query = f\"{state['ticket'].subject} {state['ticket'].description}\"\n",
        "\n",
        "            # Add reviewer feedback to query if available for refinement\n",
        "            if state.get(\"reviewer_feedback\"):\n",
        "                query += f\" {state['reviewer_feedback']}\"\n",
        "\n",
        "            # Retrieve relevant documents\n",
        "            docs = kb.similarity_search(query, k=3)\n",
        "            context = [doc.page_content for doc in docs]\n",
        "\n",
        "            logger.info(f\"Retrieved {len(context)} context items\")\n",
        "\n",
        "            return {\n",
        "                **state,\n",
        "                \"context\": context\n",
        "            }\n",
        "\n",
        "        except Exception as e:\n",
        "            logger.error(f\"Context retrieval failed: {e}\")\n",
        "            return {**state, \"context\": []}\n",
        "\n",
        "    def draft_response(self, state: AgentState) -> Dict[str, Any]:\n",
        "        \"\"\"Generate initial draft response using ticket and context\"\"\"\n",
        "        logger.info(\"Drafting response...\")\n",
        "\n",
        "        # Include previous feedback if this is a retry\n",
        "        feedback_section = \"\"\n",
        "        if state.get(\"reviewer_feedback\"):\n",
        "            feedback_section = f\"\\n\\nPREVIOUS REVIEWER FEEDBACK (address these points):\\n{state['reviewer_feedback']}\"\n",
        "\n",
        "        draft_prompt = ChatPromptTemplate.from_messages([\n",
        "            (\"system\", f\"\"\"You are a professional customer support agent. Generate a helpful, accurate response to the customer's ticket.\n",
        "\n",
        "GUIDELINES:\n",
        "- Be professional and empathetic\n",
        "- Use the provided context to inform your response\n",
        "- Don't make promises you can't keep\n",
        "- For billing issues, mention manager approval may be needed for refunds\n",
        "- For security issues, be cautious and don't reveal sensitive details\n",
        "- If you can't fully resolve the issue, explain next steps\n",
        "- Keep responses concise but complete\n",
        "\n",
        "TICKET CATEGORY: {state['classification']}\n",
        "\n",
        "AVAILABLE CONTEXT:\n",
        "{chr(10).join(state['context']) if state['context'] else 'No specific context available'}\n",
        "{feedback_section}\n",
        "\"\"\"),\n",
        "            (\"human\", \"Subject: {subject}\\nDescription: {description}\")\n",
        "        ])\n",
        "\n",
        "        try:\n",
        "            response = self.llm.invoke(\n",
        "                draft_prompt.format_messages(\n",
        "                    subject=state[\"ticket\"].subject,\n",
        "                    description=state[\"ticket\"].description\n",
        "                )\n",
        "            )\n",
        "\n",
        "            draft = response.content.strip()\n",
        "            logger.info(\"Draft response generated successfully\")\n",
        "\n",
        "            return {\n",
        "                **state,\n",
        "                \"draft_response\": draft\n",
        "            }\n",
        "\n",
        "        except Exception as e:\n",
        "            logger.error(f\"Draft generation failed: {e}\")\n",
        "            return {\n",
        "                **state,\n",
        "                \"draft_response\": \"I apologize, but I'm having trouble generating a response right now. Please contact our support team directly.\"\n",
        "            }\n",
        "\n",
        "    def review_draft(self, state: AgentState) -> Dict[str, Any]:\n",
        "        \"\"\"Review the draft response for quality and policy compliance\"\"\"\n",
        "        logger.info(\"Reviewing draft response...\")\n",
        "\n",
        "        review_prompt = ChatPromptTemplate.from_messages([\n",
        "            (\"system\", \"\"\"You are a quality assurance reviewer for customer support responses.\n",
        "\n",
        "Evaluate the draft response against these criteria:\n",
        "1. ACCURACY: Is the information correct and helpful?\n",
        "2. POLICY COMPLIANCE:\n",
        "   - No unauthorized refund promises\n",
        "   - Security issues handled appropriately (not too detailed)\n",
        "   - Professional tone maintained\n",
        "3. COMPLETENESS: Does it address the customer's main concern?\n",
        "4. CLARITY: Is it easy to understand?\n",
        "\n",
        "Respond with either:\n",
        "- \"APPROVED\" if the response meets all criteria\n",
        "- \"REJECTED: [specific feedback]\" if it needs improvement\n",
        "\n",
        "Be specific about what needs to be fixed if rejecting.\"\"\"),\n",
        "            (\"human\", \"\"\"TICKET CATEGORY: {category}\n",
        "ORIGINAL TICKET: {subject} - {description}\n",
        "DRAFT RESPONSE: {draft}\"\"\")\n",
        "        ])\n",
        "\n",
        "        try:\n",
        "            response = self.llm.invoke(\n",
        "                review_prompt.format_messages(\n",
        "                    category=state[\"classification\"],\n",
        "                    subject=state[\"ticket\"].subject,\n",
        "                    description=state[\"ticket\"].description,\n",
        "                    draft=state[\"draft_response\"]\n",
        "                )\n",
        "            )\n",
        "\n",
        "            review_content = response.content.strip()\n",
        "\n",
        "            if review_content.startswith(\"APPROVED\"):\n",
        "                review_result = {\n",
        "                    \"status\": \"approved\",\n",
        "                    \"feedback\": \"Response approved\"\n",
        "                }\n",
        "                logger.info(\"Draft approved by reviewer\")\n",
        "            else:\n",
        "                review_result = {\n",
        "                    \"status\": \"rejected\",\n",
        "                    \"feedback\": review_content.replace(\"REJECTED:\", \"\").strip()\n",
        "                }\n",
        "                logger.info(f\"Draft rejected: {review_result['feedback']}\")\n",
        "\n",
        "            return {\n",
        "                **state,\n",
        "                \"review_result\": review_result,\n",
        "                \"reviewer_feedback\": review_result[\"feedback\"] if review_result[\"status\"] == \"rejected\" else \"\"\n",
        "            }\n",
        "\n",
        "        except Exception as e:\n",
        "            logger.error(f\"Review failed: {e}\")\n",
        "            # Default to approval on error to prevent infinite loops\n",
        "            return {\n",
        "                **state,\n",
        "                \"review_result\": {\"status\": \"approved\", \"feedback\": \"Review completed with fallback\"},\n",
        "                \"reviewer_feedback\": \"\"\n",
        "            }\n",
        "\n",
        "    def refine_context(self, state: AgentState) -> Dict[str, Any]:\n",
        "        \"\"\"Refine context based on reviewer feedback\"\"\"\n",
        "        logger.info(\"Refining context based on feedback...\")\n",
        "\n",
        "        # Increment review attempts\n",
        "        new_attempts = state.get(\"review_attempts\", 0) + 1\n",
        "\n",
        "        # The retrieve_context function will use the reviewer_feedback\n",
        "        # to refine the search query, so we just need to call it again\n",
        "        updated_state = {**state, \"review_attempts\": new_attempts}\n",
        "        return self.retrieve_context(updated_state)\n",
        "\n",
        "    def should_retry_or_finalize(self, state: AgentState) -> str:\n",
        "        \"\"\"Conditional logic to determine next step after review\"\"\"\n",
        "        review_result = state.get(\"review_result\", {})\n",
        "        review_attempts = state.get(\"review_attempts\", 0)\n",
        "\n",
        "        if review_result.get(\"status\") == \"approved\":\n",
        "            return \"finalize\"\n",
        "        elif review_attempts >= MAX_REVIEW_ATTEMPTS:\n",
        "            return \"escalate\"\n",
        "        else:\n",
        "            return \"retry\"\n",
        "\n",
        "    def finalize_response(self, state: AgentState) -> Dict[str, Any]:\n",
        "        \"\"\"Finalize the approved response\"\"\"\n",
        "        logger.info(\"Finalizing approved response\")\n",
        "\n",
        "        return {\n",
        "            **state,\n",
        "            \"final_response\": state[\"draft_response\"],\n",
        "            \"escalated\": False\n",
        "        }\n",
        "\n",
        "    def escalate_ticket(self, state: AgentState) -> Dict[str, Any]:\n",
        "        \"\"\"Escalate ticket after max review attempts\"\"\"\n",
        "        logger.info(\"Escalating ticket to human review\")\n",
        "\n",
        "        # Create escalation entry\n",
        "        escalation_data = {\n",
        "            \"ticket_id\": state[\"ticket\"].ticket_id or f\"ticket_{datetime.now().strftime('%Y%m%d_%H%M%S')}\",\n",
        "            \"timestamp\": datetime.now().isoformat(),\n",
        "            \"subject\": state[\"ticket\"].subject,\n",
        "            \"description\": state[\"ticket\"].description,\n",
        "            \"classification\": state[\"classification\"],\n",
        "            \"failed_drafts\": [state[\"draft_response\"]],\n",
        "            \"reviewer_feedback\": state.get(\"reviewer_feedback\", \"\"),\n",
        "            \"review_attempts\": state.get(\"review_attempts\", 0)\n",
        "        }\n",
        "\n",
        "        # Log to CSV file\n",
        "        self._log_escalation(escalation_data)\n",
        "\n",
        "        # Generate escalation message\n",
        "        escalation_response = f\"\"\"I apologize, but I'm unable to provide a satisfactory response to your inquiry at this time.\n",
        "\n",
        "Your ticket (#{escalation_data['ticket_id']}) has been escalated to our human support team for specialized assistance.\n",
        "A team member will review your case and respond within 24 hours.\n",
        "\n",
        "Thank you for your patience.\"\"\"\n",
        "\n",
        "        return {\n",
        "            **state,\n",
        "            \"final_response\": escalation_response,\n",
        "            \"escalated\": True\n",
        "        }\n",
        "\n",
        "    def _log_escalation(self, escalation_data: Dict[str, Any]):\n",
        "        \"\"\"Log escalated ticket to CSV file\"\"\"\n",
        "        try:\n",
        "            file_exists = os.path.isfile(\"escalation_log.csv\")\n",
        "\n",
        "            with open(\"escalation_log.csv\", \"a\", newline=\"\", encoding=\"utf-8\") as csvfile:\n",
        "                fieldnames = [\"ticket_id\", \"timestamp\", \"subject\", \"description\",\n",
        "                             \"classification\", \"failed_drafts\", \"reviewer_feedback\", \"review_attempts\"]\n",
        "                writer = csv.DictWriter(csvfile, fieldnames=fieldnames)\n",
        "\n",
        "                if not file_exists:\n",
        "                    writer.writeheader()\n",
        "\n",
        "                # Convert lists to JSON strings for CSV storage\n",
        "                escalation_data[\"failed_drafts\"] = json.dumps(escalation_data[\"failed_drafts\"])\n",
        "                writer.writerow(escalation_data)\n",
        "\n",
        "            logger.info(f\"Escalation logged for ticket {escalation_data['ticket_id']}\")\n",
        "\n",
        "        except Exception as e:\n",
        "            logger.error(f\"Failed to log escalation: {e}\")\n",
        "\n",
        "    def process_ticket(self, subject: str, description: str, ticket_id: str = None) -> Dict[str, Any]:\n",
        "        \"\"\"Main method to process a support ticket\"\"\"\n",
        "        logger.info(f\"Processing new ticket: {subject}\")\n",
        "\n",
        "        # Create ticket object\n",
        "        ticket = SupportTicket(\n",
        "            subject=subject,\n",
        "            description=description,\n",
        "            ticket_id=ticket_id or f\"ticket_{datetime.now().strftime('%Y%m%d_%H%M%S')}\",\n",
        "            timestamp=datetime.now().isoformat()\n",
        "        )\n",
        "\n",
        "        # Initial state\n",
        "        initial_state = AgentState(\n",
        "            ticket=ticket,\n",
        "            classification=\"\",\n",
        "            context=[],\n",
        "            draft_response=\"\",\n",
        "            review_result={},\n",
        "            review_attempts=0,\n",
        "            final_response=\"\",\n",
        "            escalated=False,\n",
        "            reviewer_feedback=\"\"\n",
        "        )\n",
        "\n",
        "        try:\n",
        "            # Execute the graph\n",
        "            result = self.graph.invoke(initial_state)\n",
        "\n",
        "            logger.info(\"Ticket processing completed successfully\")\n",
        "            return {\n",
        "                \"success\": True,\n",
        "                \"ticket_id\": ticket.ticket_id,\n",
        "                \"classification\": result[\"classification\"],\n",
        "                \"final_response\": result[\"final_response\"],\n",
        "                \"escalated\": result[\"escalated\"],\n",
        "                \"review_attempts\": result[\"review_attempts\"]\n",
        "            }\n",
        "\n",
        "        except Exception as e:\n",
        "            logger.error(f\"Ticket processing failed: {e}\")\n",
        "            return {\n",
        "                \"success\": False,\n",
        "                \"error\": str(e),\n",
        "                \"ticket_id\": ticket.ticket_id\n",
        "            }\n",
        "\n",
